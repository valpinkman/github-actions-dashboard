{
  "name": "rescript-project-template",
  "namespace": true,
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".mjs",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "bs-dependencies": [
    "@rescript/react",
    "@glennsl/rescript-fetch",
    "@ryyppy/rescript-promise"
  ],
  "bsc-flags": []
}
